{
  "name": "wauble-starter-theme",
  "description": "A Wordpress starter theme with Laravel Vite Plugin, Tailwind, SCSS, and AlpineJS.",
  "version": "1.0.0",
  "scripts": {
    "start": "vite --config ./config/vite.config.js",
    "dev": "vite --config ./config/vite.config.js",
    "build": "vite build --config ./config/vite.config.js",
    "serve": "vite preview --config ./config/vite.config.js"
  },
  "devDependencies": {
    "@alpinejs/collapse": "^3.13.7",
    "@alpinejs/focus": "^3.13.7",
    "@alpinejs/intersect": "^3.13.7",
    "@alpinejs/morph": "^3.13.7",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "alpinejs": "^3.13.7",
    "autoprefixer": "^10.4.18",
    "laravel-vite-plugin": "^1.0.2",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-debug-screens": "^2.2.1",
    "vite": "^5.1.6"
  },
  "type": "module"
}
