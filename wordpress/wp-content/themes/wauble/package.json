{
  "name": "wauble-starter-theme",
  "description": "A Wordpress starter theme with Laravel Vite Plugin, Tailwind, SCSS, and AlpineJS.",
  "version": "1.0.0",
  "scripts": {
    "start": "vite --config ./config/vite.config.js",
    "dev": "vite --config ./config/vite.config.js",
    "build": "vite build --config ./config/vite.config.js",
    "serve": "vite preview --config ./config/vite.config.js"
  },
  "devDependencies": {
    "@alpinejs/collapse": "^3.13.2",
    "@alpinejs/focus": "^3.13.2",
    "@alpinejs/intersect": "^3.13.2",
    "@alpinejs/morph": "^3.13.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "alpinejs": "^3.13.2",
    "autoprefixer": "^10.4.16",
    "laravel-vite-plugin": "^0.8.1",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "tailwindcss-debug-screens": "^2.2.1",
    "vite": "^4.5.0"
  }
}
